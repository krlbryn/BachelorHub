<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>AddElectionServlets</servlet-name>
        <servlet-class>com.elections.servlets.AddElectionServlets</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubmitVoteServlet</servlet-name>
        <servlet-class>com.elections.servlets.SubmitVoteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddElectionServlets</servlet-name>
        <url-pattern>/AddElectionServlets</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitVoteServlet</servlet-name>
        <url-pattern>/SubmitVoteServlet</url-pattern>
    </servlet-mapping>
</web-app>
